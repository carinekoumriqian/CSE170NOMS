{
	"recipes": [
		{ "name": "Scrambled Eggs",
			"imageURL": "https://www.theflavorbender.com/wp-content/uploads/2018/08/Scrambled-Eggs-Featured-500x375.jpg",
			"id": "recipe1",
			"difficulty": "easy",
			"time": "5 minutes",
			"servingsize": 2,

			"ingredients": [
				{	"name": "regular",
					"ingredients": [
					{"ingredient":"4 eggs"},
					{"ingredient":"1/4 cup milk"},
					{"ingredient":"2 tsp butter"},
					{"ingredient":"salt and pepper"}
				] },

				{	"name": "vegan",
					"ingredients": [
					{"ingredient":"4 vegan eggs"},
					{"ingredient":"1/4 cup vegan milk"},
					{"ingredient":"2 tsp vegan butter"},
					{"ingredient":"salt and pepper"}
				] },

				{	"name": "nutfree",
					"ingredients": [
					{"ingredient":"4 nutfreeeggs"},
					{"ingredient":"1/4 cup nutfree milk"},
					{"ingredient":"2 tsp butter"},
					{"ingredient":"salt and pepper"}
				] },

				{   "name": "glutenfree",
					"ingredients": [
					{"ingredient":"4 glutenfree eggs"},
					{"ingredient":"1/4 cup glutenfree milk"},
					{"ingredient":"2 tsp glutenfree butter"},
					{"ingredient":"salt and pepper"}
					] 
				}
			],
			"allergies":[
				{"allergy":"dairy"}
			],
			"instructions": [
				{"instruction": "Beat eggs, milk, salt and pepper in medium bowl until blended."},
				{"instruction": "Heat butter in large nonstick skillet over medium heat until hot."},
				{"instruction": "Pour in egg mixture. As eggs begin to set, Gently pull the eggs across the pan with a spatula, forming large soft curds."},
				{"instruction": "Continue cooking – pulling, lifting and folding eggs – until thickened and no visible liquid egg remains. Do not stir constantly."},
				{"instruction": "Remove from heat and serve immediately."}
			]
		},
		{ "name": "Molten Caramel Cakes",
			"imageURL": "https://assets.bonappetit.com/photos/5c5d93700213ca4d2973b58a/16:9/w_2560,c_limit/molten-caramel-cake-1.jpg",
			"id": "recipe2",
			"difficulty": "hard",
			"time": "2 hours",
			"servingsize": 4,
			"normalingredients": [
				{"ingredient": "1/4 cup coarsely chopped pecans"},
				{"ingredient": "1/4 cup butter"},
				{"ingredient": "3 ounces cream cheese, softened"},
				{"ingredient": "1/3 cup firmly packed brown sugar"},
				{"ingredient": "3 large eggs"},
				{"ingredient": "1 tsp vanilla"},
				{"ingredient": "1/4 cup all-purpose flour"}
			],
			"veganingredients": [
				{"ingredient": "1/4 cup coarsely chopped vegan pecans"},
				{"ingredient": "1/4 cup vegan butter"},
				{"ingredient": "3 ounces vegan cream cheese, softened"},
				{"ingredient": "1/3 cup firmly packed vegan brown sugar"},
				{"ingredient": "3 large vegan eggs"},
				{"ingredient": "1 tsp vegan vanilla"},
				{"ingredient": "1/4 cup all-purpose vegan flour"}
			],
			"nutfreeingredients": [
				{"ingredient": "1/4 cup coarsely chopped nutfree pecans"},
				{"ingredient": "1/4 cup nutfree butter"},
				{"ingredient": "3 ounces nutfree cream cheese, softened"},
				{"ingredient": "1/3 cup firmly packed nutfree brown sugar"},
				{"ingredient": "3 large nutfree eggs"},
				{"ingredient": "1 tsp nutfree vanilla"},
				{"ingredient": "1/4 cup all-purpose nutfree flour"}
			],
			"glutenfreeingredients": [
				{"ingredient": "1/4 cup coarsely chopped gluten free pecans"},
				{"ingredient": "1/4 cup gluten free butter"},
				{"ingredient": "3 ounces gluten free cream cheese, softened"},
				{"ingredient": "1/3 cup firmly packed gluten free brown sugar"},
				{"ingredient": "3 large gluten free eggs"},
				{"ingredient": "1 tsp gluten free vanilla"},
				{"ingredient": "1/4 cup all-purpose gluten free flour"}
			],
			"allergies":[
				{"allergy":"dairy"},
				{"allergy":"tree nuts"}
			],
			"instructions": [
				{"instruction": "Heat oven to 400°F."},
				{"instruction": "Grease 4 (6-ounce) custard cups; place onto ungreased baking sheet."},
				{"instruction": "Sprinkle pecans into bottom of each cup; set aside."},
				{"instruction": "Combine chocolate chips and butter in bowl. Microwave 45-60 seconds, stirring twice, until melted and smooth."},
				{"instruction": "Add cream cheese; beat at medium speed until smooth. Set aside."},
				{"instruction": "Combine brown sugar, eggs and vanilla in bowl; beat at medium speed until well mixed."},
				{"instruction": "Add chocolate mixture; continue beating until well mixed."},
				{"instruction": "Add flour; beat at low speed until well mixed."},
				{"instruction": "Divide batter evenly among prepared custard cups. Bake 13-15 minutes or until top is puffy and cracked in appearance but center is soft. Cool 5 minutes."},
				{"instruction": "Gently run small knife around outside edges of cakes."},
				{"instruction": "Carefully invert each warm cake onto small serving plate. Serve immediately."}
			]
		},
