<html>
<head>
	<title>Test Page</title>
	<meta charset="utf-8">
	<meta name="viewport" content="width=device-width, initial-scale=1.0">
	<link href="/css/bootstrap.min.css" rel="stylesheet">
	<link href="/css/bootstrap-theme.min.css" rel="stylesheet">
	<link href="/css/homepage.css" rel="stylesheet">
	<link href="/css/global.css" rel="stylesheet">
	<link href="/css/settings.css" rel="stylesheet">
</head>
<body>
	<div class="main">
		<div class="header-back">
			<button><img class="back-button" src="https://img.icons8.com/cotton/100/000000/circled-left-2.png" onclick="goBack()"></button>

			<script>
			function goBack() {
			  window.history.back();
			}
			</script>

			<h1>Settings</h1>
		</div>
		<div>
			{{#each setting-list}}
			<div class="dropdown-settings">
				<button id="{{id}}" class="dropbtn">{{setting}}</button>
				<div id="{{id}}" class="dropdown-content">
					{{#each options}}
					<a id ="{{option}}" href="#">{{option}}</a>
					{{/each}}
				</div>
			</div>
			{{/each}}
		</div>
	</div>
	<div class="footer">
		<div class="buttons">
			<a href="homepage">
				<button > Home </button>
			</a>
			<a href="profile">
				<button> Profile </button>
			</a>
		</div>
	</div>

	<script src="https://code.jquery.com/jquery.js"></script>
	<script src="/js/bootstrap.js"></script>
	<script src="/js/settings.js"></script>

</body>
</html>
